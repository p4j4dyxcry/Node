<Application x:Class="TsNodeApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TsNodeApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--NetworkViewのテンプレートをインポート-->
                <ResourceDictionary Source="/TsNode;component/Template.xaml" />
                
                <!--サンプルをインポート-->
                <ResourceDictionary Source="/TsNode;component/Preset/Presettemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
